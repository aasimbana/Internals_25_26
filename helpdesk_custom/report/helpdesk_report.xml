<odoo>
    <record id="paperformat_helpdesk_landscape" model="report.paperformat">
      <field name="name">Helpdesk Landscape A4</field>
      <field name="default" eval="False"/>
      <field name="format">A4</field>
      <field name="orientation">Landscape</field>
      <field name="margin_top">35</field>
      <field name="margin_bottom">25</field>
      <field name="margin_left">10</field>
      <field name="margin_right">10</field>
    </record>

    <record id="action_report_helpdesk_pdf" model="ir.actions.report">
      <field name="name">Helpdesk Report PDF</field>
      <field name="model">helpdesk.report.wizard</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">helpdesk_custom.report_helpdesk_pdf</field>
      <field name="report_file">helpdesk_custom.report_helpdesk_pdf</field>
      <field name="binding_model_id" ref="model_helpdesk_report_wizard"/>
      <field name="binding_type">report</field>
      <field name="paperformat_id" ref="paperformat_helpdesk_landscape"/>
    </record>
  </odoo>
  